{
  "server": {
    "port": "8080",
    "environment": "development"
  },
  "database": {
    "host": "localhost",
    "port": "5432",
    "user": "postgres",
    "password": "postgres",
    "dbname": "traveler",
    "sslmode": "disable"
  },
  "redis": {
    "host": "localhost",
    "port": "6379",
    "password": "",
    "db": 0
  },
  "jwt": {
    "secret": "your-secret-key-change-in-production",
    "expires_in": 24
  },
  "logging": {
    "level": "debug",
    "format": "json",
    "output": "console,file",
    "file_path": "./logs/traveler_agent.log"
  },
  "cors": {
    "allowed_origins": ["http://localhost:3000"],
    "allowed_methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowed_headers": ["Origin", "Content-Type", "Accept", "Authorization"],
    "expose_headers": ["Content-Length"],
    "allow_credentials": true,
    "max_age": 86400
  }
} 